{
  "buildCommand": "",
  "outputDirectory": ".",
  "routes": [
    { "src": "/sw.js", "headers": { "Cache-Control": "no-cache", "Service-Worker-Allowed": "/" } },
    { "src": "/manifest.json", "headers": { "Content-Type": "application/manifest+json" } },
    { "src": "/parish_data.json", "headers": { "Cache-Control": "public, max-age=300" } },
    { "src": "/icon-(.*).png", "dest": "/icon-$1.png" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
